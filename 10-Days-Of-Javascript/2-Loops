/*
 * Complete the vowelsAndConsonants function.
 * Print your output using 'console.log()'.
 */
function vowelsAndConsonants(s) {
    
    let vowel = '';
    let conso = '';
    
    for(let i = 0; i< s.length; i++) {
        if(s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u') {
            vowel+=s[i]+"\n";
        } else {
            conso+=s[i]+"\n";
        }       
        
    }
    
    console.log(vowel + conso);
}