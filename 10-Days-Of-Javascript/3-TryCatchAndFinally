/*
 * Complete the reverseString function
 * Use console.log() to print to stdout.
 */
function reverseString(s) {
     
    try{
        if(Number.isInteger(s)){
        throw 's.split is not a function';
        } else{
        const a = s.split('').reverse().join('');
        console.log(a);
        }
    }
    catch(err) {
        console.log(err);
        console.log(s);
    }
}